{
	"compilerOptions": {
		"target": "ESNext",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"types": [
			"vite/client"
		],
		"allowJs": false,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"esModuleInterop": false,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"module": "ESNext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react-jsx"
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules"
	]
}